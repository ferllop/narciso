{
  "name": "scrapingwithpuppeteer",
  "version": "1.0.0",
  "description": "",
  "main": "./build/index.js",
  "type": "module",
  "scripts": {
    "start": "tsx ./src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "tsc:watch": "tsc --noEmit --watch",
    "check-config": "tsc --noEmit ./config.ts",
    "\n# TESTING": "",
    "test:args": "tsx --test --test-reporter spec",
    "test:files": "echo './test/**/**/*.test.ts'",
    "test": "npm run test:args $(npm run test:files)",
    "test:watch": "npm run test:args -- --watch $(npm run test:files)"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "puppeteer": "22.7.1"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "globals": "15.1.0",
    "tsx": "4.7.3",
    "typescript": "5.4.5"
  }
}
